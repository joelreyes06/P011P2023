<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="P011P2023.Views.Page_Two">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding nombres}"
             
                HorizontalOptions="CenterAndExpand" x:Name="lblnombre" />

            <Label Text="{Binding apellidos}"
              
                HorizontalOptions="CenterAndExpand" x:Name="lblapellidos" />

            <Label Text="{Binding telefono}"
               
                HorizontalOptions="CenterAndExpand" x:Name="lbltelefono" />
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>